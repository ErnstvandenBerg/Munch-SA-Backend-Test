{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "no-console": [
            true,
            "logs"
        ]
    },
    "rules": {
        "trailing-comma": [
            false
        ],
        "no-console": [
            true,
            "logs"
        ],
        "prefer-for-of": false,
        "forin": false,
        "no-shadowed-variable": false,
        "only-arrow-functions": false,
        "variable-name": false,
        "no-var-requires": false,
        "class-name": false,
        "no-unused-expression": false
    },
    "rulesDirectory": []
}